<div id="fh5co-timeline">
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-md-offset-0">
                <ul class="timeline animate-box">
                    <li class="timeline-heading text-center animate-box">
                        <div><h3>Your Infomation</h3></div>
                    </li>
                    <li class="animate-box timeline-unverted">
                        <div class="timeline-badge"><i class="icon-genius"></i></div>
                        <div class="timeline-panel" style="margin-top: 24px">
                            <div class="timeline-heading">
                                <h3 class="timeline-title">Date of birth</h3>

                            </div>
                            <div class="timeline-body">
                                <p>{{dateFormat user.info.date_of_birth format="DD MMMM - YYYY"}}</p>
                            </div>
                        </div>
                    </li>
                    <li class="timeline-inverted animate-box">
                        <div class="timeline-badge"><i class="icon-genius"></i></div>
                        <div class="timeline-panel" style="margin-top: 24px">
                            <div class="timeline-heading">
                                <h3 class="timeline-title">Gender</h3>
                            </div>
                            <div class="timeline-body">
                                <p>{{user.info.sex}}</p>
                            </div>
                        </div>
                    </li>
                    <li class="animate-box timeline-unverted">
                        <div class="timeline-badge"><i class="icon-genius"></i></div>
                        <div class="timeline-panel" style="margin-top: 24px">
                            <div class="timeline-heading">
                                <h3 class="timeline-title">Email</h3>
                            </div>
                            <div class="timeline-body">
                                <p>{{user.info.email}}</p>
                            </div>
                        </div>
                    </li>
                    <li class="animate-box timeline-inverted">
                        <div class="timeline-badge"><i class="icon-genius"></i></div>
                        <div class="timeline-panel" style="margin-top: 24px">
                            <div class="timeline-heading">
                                <h3 class="timeline-title">Phone</h3>
                            </div>
                            <div class="timeline-body">
                                <p>{{user.info.phone_number}}</p>
                            </div>
                        </div>
                    </li>
                    <li class="animate-box timeline-unverted">
                        <div class="timeline-badge"><i class="icon-genius"></i></div>
                        <div class="timeline-panel" style="margin-top: 24px">
                            <div class="timeline-heading">
                                <h3 class="timeline-title">Address</h3>
                            </div>
                            <div class="timeline-body">
                                <p>{{user.info.address}}</p>
                            </div>
                        </div>
                    </li>
                    {{#if user.info.ssn}}
                    <li class="animate-box timeline-inverted">
                        <div class="timeline-badge"><i class="icon-genius"></i></div>
                        <div class="timeline-panel" style="margin-top: 24px">
                            <div class="timeline-heading">
                                <h3 class="timeline-title">Social Security number</h3>
                            </div>
                            <div class="timeline-body">
                                <p>{{user.info.ssn}}</p>
                            </div>
                        </div>
                    </li>
                    {{/if}}
                    {{#if user.info.citizenship}}
                    {{#if user.info.ssn}}
                    <li class="animate-box timeline-unverted">
                    {{else}}
                    <li class="animate-box timeline-inverted">
                    {{/if}}
                        <div class="timeline-badge"><i class="icon-genius"></i></div>
                        <div class="timeline-panel" style="margin-top: 24px">
                            <div class="timeline-heading">
                                <h3 class="timeline-title">Citizenship</h3>
                            </div>
                            <div class="timeline-body">
                                <p>{{user.info.citizenship}}</p>
                            </div>
                        </div>
                    </li>
                    {{/if}}
                    <li class="timeline-heading text-center animate-box">
                        <div>
                            <p style="display: inline-block; margin: 40px 0 0 0;"><a href="/update" style="background: #ec3e44; margin: 12px;" class="btn btn-primary btn-outline">Edit Profile</a></p>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>